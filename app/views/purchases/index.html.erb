<div>
  <h2>Your Songs</h2>
</div>
<table class="table">
  <tr>
    <th>Song</th>
    <th></th>
  </tr>

<% @songs.each do |s| %>
  <tr>
    <td><%= link_to s.name, song_path(s) %></td>>
    <td><%= audio_tag(s.song_file, :controls => true) if s.song_file.present? %></td>
  </tr>
<% end %>
</table>

<%= link_to "Add a Song", new_song_path %>
