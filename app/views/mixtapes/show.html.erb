<h2>Show Mixtape</h2>

<%= @mixtape.name %>
<br/>
Playlist
<ul>
  <% @mixtape.songs.each do |s| %>
    <li><%= s.name %><br/>
    <%= audio_tag(s.song_file, :controls => true) if s.song_file.present? %>
    </li>
  <% end %>
<ul>
